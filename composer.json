{
    "name": "celionatti/neoliens",
    "autoload": {
        "psr-4": {
            "Neoliens\\": "./"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "authors": [
        {
            "name": "celionatti",
            "email": "amisuusman@gmail.com"
        }
    ],
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^1.6",
        "laminas/laminas-diactoros": "^3.0",
        "twig/twig": "^3.6",
        "symfony/webpack-encore-bundle": "^2.0",
        "slim/twig-view": "^3.3",
        "symfony/twig-bridge": "^6.3",
        "twig/intl-extra": "^3.6",
        "vlucas/phpdotenv": "^5.5",
        "vlucas/valitron": "^1.4",
        "slim/csrf": "^1.3",
        "php-di/php-di": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    }
}
